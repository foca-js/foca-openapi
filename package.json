{
  "name": "foca-openapi",
  "version": "0.7.0",
  "description": "根据openapi文档生成请求客户端",
  "scripts": {
    "test": "vitest",
    "generate": "aomex openapi-yaml && aomex openapi-json",
    "build": "tsup && sh scripts/build-success.sh",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "deploy:main": "sh scripts/develop-deploy-main.sh",
    "sync:main": "sh scripts/develop-sync-main.sh"
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./adapters/fetch": {
      "types": "./adapters/fetch.d.ts",
      "import": "./adapters/esm/fetch.js",
      "require": "./adapters/fetch.js"
    },
    "./adapters/axios": {
      "types": "./adapters/axios.d.ts",
      "import": "./adapters/esm/axios.js",
      "require": "./adapters/axios.js"
    },
    "./adapters/taro": {
      "types": "./adapters/taro.d.ts",
      "import": "./adapters/esm/taro.js",
      "require": "./adapters/taro.js"
    },
    "./adapters/uniapp": {
      "types": "./adapters/uniapp.d.ts",
      "import": "./adapters/esm/uniapp.js",
      "require": "./adapters/uniapp.js"
    },
    "./package.json": "./package.json"
  },
  "bin": "dist/bin.mjs",
  "type": "module",
  "publishConfig": {
    "type": "commonjs",
    "access": "public"
  },
  "files": [
    "backup",
    "adapters",
    "dist",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [],
  "repository": "git@github.com:foca-js/foca-openapi.git",
  "homepage": "https://foca.js.org",
  "license": "MIT",
  "volta": {
    "node": "20.13.0",
    "pnpm": "9.5.0"
  },
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": "^18.19 || ^20.6 || >=22"
  },
  "peerDependencies": {
    "typescript": ">=5.4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "listr2": "^8.2.3",
    "lodash-es": "^4.17.21",
    "object-to-formdata": "^4.5.1",
    "openapi-types": "^12.1.3",
    "prettier": "^3.2.5",
    "query-string": "^9.0.0",
    "tsx": "^4.16.2",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@aomex/console": "^2.0.2",
    "@aomex/core": "^2.0.2",
    "@aomex/openapi": "^2.0.2",
    "@aomex/web": "^2.0.2",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@dcloudio/types": "^3.4.12",
    "@release-it/conventional-changelog": "^8.0.1",
    "@tarojs/taro": "^3.6.34",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.10",
    "@types/qs": "^6.9.15",
    "@vitest/coverage-v8": "^2.0.3",
    "axios": "^1.7.2",
    "husky": "^9.0.11",
    "only-allow": "^1.2.1",
    "release-it": "^17.6.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  }
}
